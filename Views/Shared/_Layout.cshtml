<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AutoFix</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/AutoFix.styles.css" asp-append-version="true" />
    <link href="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
</head>
<body>
    <header>
        <div class="logo-container">
             <a asp-controller="Home" asp-action="Index"><img class="img-logo" src="/images/logo_medium.png"></a>
        </div>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light header-footer-bg border-bottom" style="height: 80px;">
            <div class="container-fluid">

                @* <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><img src="/images/logo_medium.png" class="logo-medium"></a> *@
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1 justify-content-end">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Bookings" asp-action="Create">Boka tjänst <img src="/images/gear-solid.svg" class="nav-icon"></a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Bookings" asp-action="Index">Mina bokningar <img src="/images/circle-user-solid.svg" class="nav-icon"></a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container-fluid gradient-bg">
        <main role="main">
            @RenderBody()
        </main>
    </div>

    <footer class="footer text-muted header-footer-bg border-top" style="z-index: 2;">
        <div class="container">
            &copy; 2022 - AutoFix - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC66ATu8ihz87_r2OhBR5W1VA79wNOI9II&libraries=places&callback=initAutocomplete" async defer></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="/js/auto-complete.js"></script>
    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>
